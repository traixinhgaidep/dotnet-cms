@model List<Models.EF.Channel>

<div class="logo">
    <a href="/Home/Index/"><img src="/Assets/Frontend/images/logo.png" class="img-responsive" alt="" /></a>
</div>
<h4 class="menn">Danh mục</h4>
<label></label>
<div class="head-nav">
    <span class="menu"> </span>
    @foreach (var item in Model)
    {
        var url = "/Channel/" + item.Name + "-" + item.IDChannel;
        <h4><a href="@url"><i class="fa fa-link"></i> <span>@item.Name</span></a></h4>
    }
</div>
<div class="clearfix"> </div>
